if [[ -s "${HOME}/.zshrc.default" ]]; then
    source "${HOME}/.zshrc.default"
fi

if [[ -s "${HOME}/.settings/zsh/aliases.zsh" ]]; then
    source "${HOME}/.settings/zsh/aliases.zsh"
fi

if [[ -s "${HOME}/.settings/zsh/exports.zsh" ]]; then
    source "${HOME}/.settings/zsh/exports.zsh"
fi

if [[ -s "${HOME}/.settings/zsh/settings.zsh" ]]; then
    source "${HOME}/.settings/zsh/settings.zsh"
fi

if [[ -s "${HOME}/.settings/zsh/completion.zsh" ]]; then
    source "${HOME}/.settings/zsh/completion.zsh"
fi
